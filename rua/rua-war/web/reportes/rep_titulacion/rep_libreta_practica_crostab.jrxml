	<queryString>
		<![CDATA[select ide_ytilpr,ide_yhodia,hora_inicio_ytihpr,descripcion,numero_horas_ytihpr, descripcion_yhodia,orden_yhodia,orden
from (
 select c.ide_ytilpr,d.ide_yhodia,c.hora_inicio_ytihpr,'HORA DESDE' as descripcion, c.numero_horas_ytihpr, d.descripcion_yhodia,orden_yhodia,1 as orden
from yavirac_titu_horario_practica c, yavirac_hora_dia d
where c.ide_yhodia=d.ide_yhodia
UNION
 select c.ide_ytilpr,d.ide_yhodia, c.hora_fin_ytihpr,'HORA HASTA' as descripcion, c.numero_horas_ytihpr, d.descripcion_yhodia,orden_yhodia,2 as orden
from yavirac_titu_horario_practica c, yavirac_hora_dia d
where c.ide_yhodia=d.ide_yhodia
 order by orden_yhodia,orden
 ) a
 where ide_ytilpr=$P{pide_libreta_practica}]]>
