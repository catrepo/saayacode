left join yavirac_alum_direccion_alumno g on a.ide_yaldap=g.ide_yaldap
left join yavirac_alum_telefono h on a.ide_yaldap=h.ide_yaldap
left join yavirac_alum_correo i on a.ide_yaldap=i.ide_yaldap
left join yavirac_alum_dato_fami_alumno j on a.ide_yaldap=j.ide_yaldap
left join yavirac_alum_gasto k on j.ide_yalgas=k.ide_yalgas
left join yavirac_alum_tipo_vivienda l on j.ide_yaltiv=l.ide_yaltiv
left join yavirac_stror_estado_civil m on j.ide_ystesc=m.ide_ystesc
left join yavirac_stror_parentezco_fami n on j.ide_ystpaf=n.ide_ystpaf
left join yavirac_alum_ocupacion o on j.ide_yalocu=o.ide_yalocu
left join yavirac_alum_ocupacion p on j.yav_ide_yalocu=p.ide_yalocu
left join yavirac_alum_ocupacion q on j.yav_ide_yalocu2=q.ide_yalocu
left join (
select ide_yaldaa,ide_yaldap,b.descripcion_ystins as instutucion1,d.descripcion_ystins as instutucion2,descripcion_yalcar as carrera,anio_graduacion_yaldaa,estudio_adicional_yaldaa, anio_ingreso_yaldaa, anio_egreso_yaldaa
from yavirac_alum_dato_academico a
left join yavirac_stror_institucion b on a.ide_ystins=b.ide_ystins
left join yavirac_alum_carrera  c on a.ide_yalcar=c.ide_yalcar
left join yavirac_stror_institucion d on a.yav_ide_ystins=d.ide_ystins
) r on a.ide_yaldap=r.ide_yaldap
left join yavirac_alum_dato_laboral s on a.ide_yaldap=s.ide_yaldap
where a.ide_yaldap=27]]>
