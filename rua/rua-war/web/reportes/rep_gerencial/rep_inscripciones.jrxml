	<queryString>
		<![CDATA[select descripcion_ystpea,ide_ystpea, descripcion_ystmen,descripcion_ystgen , COUNT (*) TOTAL
from ( SELECT a.descripcion_ystpea,a.ide_ystpea,  c.descripcion_ystmen, e.descripcion_ystgen
from yavirac_stror_periodo_academic a
inner join yavirac_ins_pre_inscripcion b on a.ide_ystpea = b.ide_ystpea
inner join yavirac_stror_mension c on b.ide_ystmen = c.ide_ystmen
inner join yavirac_alum_dato_personal d on b.ide_yaldap = d.ide_yaldap
inner join yavirac_stror_genero e on d.ide_ystgen = e.ide_ystgen
where a.ide_ystpea in ($P!{pperiodo})
and b.ide_ystmen in ($P!{pmension})
) k
group by descripcion_ystpea,descripcion_ystmen,descripcion_ystgen,ide_ystpea]]>
	</queryString>
