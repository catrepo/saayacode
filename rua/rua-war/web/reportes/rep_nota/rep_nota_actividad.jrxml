	<queryString>
		<![CDATA[select ROW_NUMBER() OVER(order by b.ide_ynodet) as numero,a.ide_ynocan,b.ide_yaldap,ide_ynodet,descripcion_ystpea,descripcion_ystmen,descripcion_ystnie,detalle_yhogra,descripcion_ystjor,
ide_ynoace,descripcion_ynoace,descripcion_ynotie,detalle_ystmat,
apellido_yaldap||' '||nombre_yaldap as alumno,doc_identidad_yaldap,fecha_calificacion_ynocan,a.detalle_ynocan,nota_ynodet,porciento_evaluacion_ynoacd,
apellido_ypedpe||' '||nombre_ypedpe as docente
from (
select ide_ynocan,a.ide_ystjor,a.ide_ystpea,a.ide_ynopae,a.ide_ystmen,a.ide_ystnie,a.ide_yhogra,a.ide_ystmal,a.ide_ypedpe,j.ide_ynotie,h.ide_ynoace
,descripcion_ystpea,descripcion_ystmen,descripcion_ystnie,apellido_ypedpe||' '||nombre_ypedpe as docente,detalle_yhogra,descripcion_ystjor,
descripcion_ynoace,descripcion_ynotie,detalle_ystmat,detalle_ynocan,fecha_calificacion_ynocan
from yavirac_nota_cabecera_nota a,yavirac_stror_periodo_academic b,yavirac_stror_mension c,yavirac_stror_nivel_educacion d,yavirac_perso_dato_personal e,
yavirac_hora_grupo_academic f,yavirac_stror_jornada g,yavirac_nota_periodo_activ_eva h,yavirac_nota_actividad_evaluac i,yavirac_nota_periodo_evaluacio j,
yavirac_nota_tipo_evaluacion k,yavirac_stror_malla l,yavirac_stror_materia m
where a.ide_ystpea=b.ide_ystpea
and a.ide_ystmen=c.ide_ystmen
and a.ide_ystnie=d.ide_ystnie
and a.ide_ypedpe=e.ide_ypedpe
and a.ide_yhogra=f.ide_yhogra
and a.ide_ystjor=g.ide_ystjor
and a.ide_ynopae=h.ide_ynopae
and h.ide_ynoace=i.ide_ynoace
and h.ide_ynopee=j.ide_ynopee
and j.ide_ynotie=k.ide_ynotie
and a.ide_ystmal=l.ide_ystmal
and l.ide_ystmat=m.ide_ystmat
) a
join yavirac_nota_detalle_nota  b  on a.ide_ynocan=b.ide_ynocan
join yavirac_alum_dato_personal c on b.ide_yaldap=c.ide_yaldap
left join yavirac_nota_actividad_docente d on a.ide_ynopae=d.ide_ynopae
left join yavirac_perso_dato_personal e on a.ide_ypedpe=e.ide_ypedpe
where a.ide_ystpea in($P!{ide_ystpea}) and a.ide_ystnie in ($P!{ide_ystnie}) and a.ide_yhogra in($P!{ide_yhogra}) and a.ide_ystmal in ($P!{ide_ystmal}) and a.ide_ypedpe in ($P!{ide_ypedpe}) and a.ide_ystjor in ($P!{ide_ystjor})
and a.ide_ynotie in ($P!{ide_ynotie}) and a.ide_ynoace in($P!{ide_ynoace})
group by ide_ynotie,ide_ynoace,a.ide_ynocan,ide_ynodet,descripcion_ystpea,descripcion_ystmen,descripcion_ystnie,detalle_yhogra,descripcion_ystjor,
descripcion_ynoace,b.ide_yaldap,descripcion_ynotie,detalle_ystmat,apellido_yaldap,nombre_yaldap,doc_identidad_yaldap,fecha_calificacion_ynocan,a.detalle_ynocan,nota_ynodet,
porciento_evaluacion_ynoacd,apellido_ypedpe,nombre_ypedpe
order by ide_ynotie,ide_ynoace,apellido_yaldap]]>
	</queryString>
